#include <bits/stdc++.h>
using namespace std;

bool arraySorted(int ind, vector<int>ve){
    if(ind==ve.size()-1){
        return true;
    }
    
    return ve[ind]<ve[ind+1] && arraySorted(ind+1, ve);
}
int main() {
	
	int n;
	cin>>n;
	vector<int>ve(n);
	for(int i=0;i<n;i++){
	    cin>>ve[i];
	}
	
	if(arraySorted(0, ve)){
	    cout<<"yes"<<endl;
	}
	else{
	    cout<<"no";
	}

}
