#include<bits/stdc++.h>
using namespace std;
vector<vector<int>>Print_Subseq(int idx,vector<int>&v,vector<int>temp,int si){
    if(idx>=si){
        v1.push_back(temp);
        return;
    }
    //pick call
    temp.push_back(v[idx]);
    Print_Subseq(idx+1,v,temp,si);
    //not pick
    temp.pop_back();
    Print_Subseq(idx+1,v,temp,si);
}
int main(){
    vector<int>v={1,2,3};
    int si=v.size();
    vector<int>tmp;
    vector<vector<int>>mp=Print_Subseq(0,v,tmp,si);
    for(int i=0;i<v1.size();i++){
        for(int j=0;j<v1[i].size();j++){
            cout<<v1[i][j]<<" ";
        }
        cout<<endl;
    }
    
}
